{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spotube-control",
  "title": "Spotube control",
  "description": "Control Spotube from Raycast",
  "icon": "command-icon.png",
  "author": "EmmanuelYee",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-play-pause",
      "title": "Toggle Play/Pause",
      "subtitle": "toggle play button",
      "description": "pauses and plays",
      "mode": "view"
    },
    {
      "name": "pause",
      "title": "Pause",
      "subtitle": "pauses",
      "description": "",
      "mode": "view"
    },
    {
      "name": "play",
      "title": "Play",
      "subtitle": "plays",
      "description": "",
      "mode": "view"
    },
    {
      "name": "previous",
      "title": "Previous",
      "subtitle": "previous",
      "description": "",
      "mode": "view"
    },
    {
      "name": "next",
      "title": "Next",
      "subtitle": "next",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}